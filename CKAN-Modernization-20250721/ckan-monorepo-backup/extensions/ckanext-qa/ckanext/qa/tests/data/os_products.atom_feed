	<entry>
		<title>OS MasterMap Integrated Transport Network Layer</title>
		<link rel="alternate" type="application/atom+xml"
			href="http://www.ordnancesurvey.co.uk/oswebsite/xml/atom/ITN.xml"/>
		<link rel="describedby" type="application/vnd.iso.19139+xml"
